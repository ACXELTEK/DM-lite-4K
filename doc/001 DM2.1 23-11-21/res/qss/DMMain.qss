
/*主界面*/

QWidget[Type="Light"]{
background: #444444;
border: 0px;
}
QFrame[Type="Dark"]{
background: #282828;
}
QFrame[Type="loseedge"]{
border:0px solid #ffffff;
}
QDialog[Type="Dark"]{
background: #282828;
}
QFrame[Type="Light"]{
background: #444444;
}



QTabWidget{
}
QTabWidget::tab-bar {
    border: 0px;
}
QTabWidget::pane{
}
QTabBar {
    qproperty-drawBase: 0;
}
QTabBar::tab {
background-color:#282828; 
border:0px solid #ffffff;
color: #FFFFFF;
width:160;
height: 36px;
font-weight: normal;
}
QTabBar::tab:hover {
border-top-right-radius: 4px;
border-bottom:2px solid #ffffff;
}
QTabBar::tab:selected {
border-top-right-radius: 4px;
background-color:#282828;
}
QTabBar::tab:!selected {
border-top-right-radius: 4px;
background-color:#444444;
}

/*温控tab*/
QTabWidget[Type = "Temp"]{
}
QTabWidget[Type = "Temp"]::tab-bar {
    border: 0px;
}
QTabWidget[Type = "Temp"]::pane{
}
QTabBar[Type = "Temp"] {
    qproperty-drawBase: 0;
}
QTabBar[Type = "Temp"]::tab {
background-color:#282828; 
border:0px solid #ffffff;
color: #FFFFFF;
width:160;
height: 36px;
font-weight: normal;
}
QTabBar[Type = "Temp"]::tab:hover {
border:0px solid #ffffff;
background-color:transparent;
}
QTabBar[Type = "Temp"]::tab:selected {
margin-bottom:20px;
border:0px solid #ffffff;
background-color:transparent;
border-bottom:2px solid #0F4C81;
}
QTabBar[Type = "Temp"]::tab:!selected {
border:0px solid #ffffff;
background-color:transparent;
}


/*按钮 tb*/
QToolButton{
border-radius: 3px;
opacity: 1;
color: #FFFFFF;
background: #282828;
}
QToolButton::pressed{
border-radius: 3px;
opacity: 1;
background: #7B7B7B;
box-sizing: border-box;
border: 0.4px solid #C1C1C1;
font-size: 12px;
font-weight: normal;
line-height: 14px;
letter-spacing: 0px;
color: #FFFFFF;
}
QToolButton:hover{
border-radius: 3px;
opacity: 1;
background: #7B7B7B;
box-sizing: border-box;
border: 0.4px solid #C1C1C1;
font-size: 12px;
font-weight: normal;
line-height: 14px;
letter-spacing: 0px;
color: #FFFFFF;
}
QToolButton:disabled{
font-size: 12px;
font-weight: normal;
line-height: 14px;
letter-spacing: 0px;
background: #444444;
border-radius: 3px;
opacity: 1;
box-sizing: border-box;
border: 0.4px solid #C1C1C1;
color: #7B7B7B;
}
QToolButton[Type = "min"]{
image: url(./res/icon/Windows/ToolButton/min.png);
}
QToolButton[Type = "max"]{
image: url(./res/icon/Windows/ToolButton/max.png);
}
QToolButton[Type = "normal"]{
image: url(./res/icon/Windows/ToolButton/normal.png);
}
QToolButton[Type = "close"]{
image: url(./res/icon/Windows/ToolButton/close.png);
}
