/*Combox*/
QComboBox{
background: #282828;
border: 1px solid #D7D9DE;
font-size: 14px;
font-weight: 300px;
line-height: 30px;
letter-spacing: 0px;
padding-left: 30px;
color: #FFFFFF;
opacity: 1;
selection-color: rgb(235,235,235);
selection-background-color: rgb(83,121,180);
}
QComboBox::drop-down
{
	subcontrol-origin: padding;
	subcontrol-position: center right;
	width: 30px;
	border:none;
}
QComboBox::down-arrow {
    image: url(./res/icon/Windows/PushButton/down.png);
}
QComboBox::down-arrow:on {
    image: url(./res/icon/Windows/PushButton/up.png);
}
QComboBox QAbstractItemView {
    outline: 0px solid #000000;
    border: 0px solid #000000;/* 整个下拉窗体的边框 */
    color: #FFFFFF;
	text-align: left 30px;
    background-color: #282828; /* 整个下拉窗体的背景色 */
    selection-color: #FFFFFF;/*下拉框选中项字体颜色*/
    selection-background-color:#7B7B7B;/* 下拉框选中项的背景色 */
}


/*滑动条*/
QSlider::groove:horizontal{
height: 12px;
left: 0px;
right: 0px;
border:0px; 
border-radius:6px; 
background:#7B7B7B;
}
QSlider::handle:horizontal{
width: 16px;
height: 12px;
margin-top: -5px;
margin-left: 0px;
margin-bottom: -5px;
margin-right: 0px;
background:#ffffff;
border-radius:11px; 
border: 3px solid #7B7B7B;
}
QSlider::sub-page:horizontal{
background:#ffffff;
border-radius:6px; 
}